            background-color: #bab6ab;
            font-weight: 500;
            letter-spacing: 1px;
            font-size: 15px;
            height: 37px;
        }

        .header-title {
            display: inline-block;
        }

        .search {
            display: inline-block;
            float: right;
            /*background-color:#bab6ab;*/
        }

        .row-fluid {
            margin: 5px 0;
        }
       .row-fluid select {
            width: 66%;
        }
       
        .custom-button {
            padding: 5px 25px;
            margin: 5px 5px;
            text-align: center;
            font-size: 16px;
            cursor: pointer;
            background-color: #7c7a72;
            color: white;
            border: 2px solid #bab6ab;
            border-radius: 5px;
            letter-spacing: 1px;
        }

            .custom-button:hover {
                background-color: #e1dcd6;
