            border: 1px solid grey;
            border-bottom: 5px solid grey;
            margin: 5px 0px;
            background-color: #e1dcd6;
        }

        .emp-detail-fields {
            margin: 19px 0px;
        }

        .field-item1 {
            height: 26px;
        }

        .field-item1 select {
            width: 177px;
            height: 26px;
        }

        .car-table-wrapper {
            border: 1px solid grey;
            border-bottom: 5px solid grey;
            width: 100%;
            display: inline-block;
            margin: 5px 0px;
        }

        .car-table {
            background-color: #e1dcd6;
            padding: 15px 10px;
            overflow-x: auto;
        }

        table {
            border-collapse: collapse;
            border-spacing: 0;
            width: 100%;
            border: 1px solid #ddd;
        }

        th, td {
            border: 1px solid grey;
            text-align: left;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #f2f2f2
        }
        .required-field::after
         {            content: "*";            color: red;        }
