            background-color: #bab6ab;
            color: white;
            font-weight: 500;
            margin-bottom: 15px;
            letter-spacing: 2px;
            font-size: 17px;
        }

        .box {
            background-color: #e1dcd6;
            padding: 15px;
            overflow-x: auto;
        }

        .row-fluid {
            margin: 5px 0;
        }

            .row-fluid label {
                margin-right: 8px;
                letter-spacing: 1px;
                width: 200px;
            }

        .custom-button {
            padding: 5px 25px;
            margin: 4px 2px;
            text-align: center;
            font-size: 16px;
            cursor: pointer;
            background-color: #bab6ab;
            color: white;
            border: 2px solid #bab6ab;
            border-radius: 5px;
            letter-spacing: 1px;
        }

            .custom-button:hover {
                background-color: lightgrey;
                color: black;
            }

        #table-wraper {
            width: 100%;
            display: inline-block;
        }

        .for-header {
            padding: 5px 10px;
            background-color: #bab6ab;
            font-weight: 400;
            letter-spacing: 1px;
            font-size: 15px;
        }

        table {
            border-collapse: collapse;
            border-spacing: 0;
            width: 100%;
            border: 1px solid #ddd;
        }

        th, td {
            border: 1px solid grey;
            text-align: left;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #f2f2f2
        }



        /*Style tab*/
        body {
            font-family: "Lato", sans-serif;
        }

        /* Style the tab */
        div.tab {
        overflow: hidden;
        border: 1px solid #ccc;
        background-color: #f1f1f1;
        }

        /* Style the buttons inside the tab */
        div.tab button {
        background-color: inherit;
        float: left;
        border: none;
        outline: none;
        cursor: pointer;
        padding: 14px 16px;
        transition: 0.3s;
        font-size: 17px;
        }

        /* Change background color of buttons on hover */
        div.tab button:hover {
        background-color: #ddd;
        }

        /* Create an active/current tablink class */
        div.tab button.active {
        background-color: #ccc;
        }

        /* Style the tab content */
        .tabcontent {
        display: none;
        padding: 6px 12px;
        border: 1px solid #ccc;
        border-top: none;
        }
        
    </style>
</head>
