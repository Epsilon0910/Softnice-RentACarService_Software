<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Rent a Car-Car Details</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <!-- CSS -->

    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Open+Sans+Condensed:700" />
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/bootstrap.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/font-awesome.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/style.css">

    <!-- Js -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/bootstrap.min.js"></script>
    <style>
        .main-header {
            padding: 5px;
            background-color: #a09d93;
            border-bottom: 1px solid black;
            -moz-border-radius-topleft: 5px;
            -moz-border-radius-topright: 5px;
            color: white;
            font-weight: 700;
            margin-bottom: 15px;
            letter-spacing: 2px;
            font-size: 20px;
            height: 40px;
        }

        .sub-header {
            padding: 5px 10px;
            background-color: #bab6ab;
            font-weight: 500;
            letter-spacing: 1px;
            font-size: 15px;
        }

        .row-fluid {
            margin: 5px 0;
        }

        .custom-button {
            padding: 5px 25px;
            margin: 5px 5px;
            text-align: center;
            font-size: 16px;
            cursor: pointer;
            background-color: #7c7a72;
            color: white;
            border: 2px solid #bab6ab;
            border-radius: 5px;
            letter-spacing: 1px;
        }

            .custom-button:hover {
                background-color: #e1dcd6;
                color: black;
            }

        .main-wrapper {
            border: 1px solid black;
            border-radius: 5px;
            margin: 50px 0;
        }

        .main {
            margin: 15px 5px;
        }

        .car-detail-fields-wrapper {
            border: 1px solid grey;
            border-bottom: 5px solid grey;
            margin: 5px 0px;
            background-color: #e1dcd6;
        }

        .car-detail-fields {
            margin: 5px 0px;
        }

        .car-table-wrapper {
            border: 1px solid grey;
            border-bottom: 5px solid grey;
            width: 100%;
            display: inline-block;
            margin: 5px 0px;
        }

        .car-table {
            background-color: #e1dcd6;
            padding: 15px 10px;
            overflow-x: auto;
        }

        table {
            border-collapse: collapse;
            border-spacing: 0;
            width: 100%;
            border: 1px solid #ddd;
        }

        th, td {
            border: 1px solid grey;
            text-align: left;
            padding: 8px;
        }

        tr:nth-child(even) {
            background-color: #f2f2f2
        }
    </style>
</head>

<body>
    <!-- Header Start -->
    <header>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <!-- header Nav Start -->
                    <nav class="navbar navbar-default">
                        <div class="container-fluid">
                            <!-- Brand and toggle get grouped for better mobile display -->
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>
                                <img src="~/Content/mytemplate/img/logo.png" />
                            </div>
                            <!-- Collect the nav links, forms, and other content for toggling -->
                            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                                <ul class="nav navbar-nav navbar-right">
                                    <li>@Html.ActionLink("Filters", "Landing")</li>
                                    <li>@Html.ActionLink("Book a Car", "Book")</li>
                                    <li>@Html.ActionLink("Logout", "Login")</li>
                                </ul>
                            </div><!-- /.navbar-collapse -->
                        </div><!-- /.container-fluid -->
                    </nav>
                </div>
            </div>
        </div>
    </header><!-- header close -->
    <section class="container">
        <div class="main-wrapper">
            <div class="main-header">
                LOGISTICS 
            </div>
            <div class="main">
                <div class="car-detail-fields-wrapper">
