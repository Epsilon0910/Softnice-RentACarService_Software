@{
    Layout = null;
}

<!DOCTYPE html>
<html class="no-js">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Rent a Car-Details</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <!-- CSS -->
    
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/")logincss.css" />
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Open+Sans+Condensed:700" />
    <style>


body {
  background: #999;
  padding: 40px;
  font-family: "Open Sans Condensed", sans-serif;
}

#bg {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  background: url(http://lorempixel.com/800/500/nature) no-repeat center center fixed;
  background-size: cover;
  -webkit-filter: blur(5px);
}

form {

  position: relative;
  width: 150px,150px;
  margin: 100px auto;
  display: inline-block;
  background: rgba(60,60,60,.35) ;
  padding: 350px 250px;
  border: 0px solid;
  border-top-color: #4caf50;
  border-left-color: #fff;
  border-bottom-color:#fff;
  border-right-color: #4caf50;
}
.btn{
      width: 250px;
background: #4caf50;

}

form input, form button {

  width: 212px;
  align-content:center;
  border: 3px solid;
  border-bottom-color: rgba(255,255,255,.5);
  border-right-color: rgba(60,60,60,.35);
  border-top-color: rgba(60,60,60,.35);
  border-left-color: rgba(80,80,80,.45);
  background-color: rgba(0,0,0,.2);
  background-repeat: no-repeat;
  padding: 8px 24px 8px 10px;
  font: bold .875em/11.25em "Open Sans Condensed", sans-serif;
  letter-spacing: .075em;
  color: #fff;
  text-shadow: 0 1px 0 rgba(0,0,0,.1);
  margin-bottom: 19px;


}

form input:focus { background-color: rgba(0,0,0,.4); }

form input.email {
  background-image: url(data:image/png;base64,);
  background-position: 220px 10px;
}

form input.pass {
  background-image: url(data:image/png;base64,);
  background-position: 223px 8px
}

::-webkit-input-placeholder { color: #ccc; text-transform: uppercase; }
::-moz-placeholder { color: #ccc; text-transform: uppercase; }
:-ms-input-placeholder { color: #ccc; text-transform: uppercase; }

form button[type=submit] {
  width: 248px;
  margin-bottom: 0;
  color: #3f898a;
  letter-spacing: .05em;
  text-shadow: 0 1px 0 #133d3e;
  text-transform: uppercase;
  background: #225556;
  border-top-color: #9fb5b5;
  border-left-color: #608586;
  border-bottom-color: #1b4849;
  border-right-color: #1e4d4e;
  cursor: pointer;
}
    </style>
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/owl.carousel.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/bootstrap.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/font-awesome.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/style.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/ionicons.min.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/animate.css">
    <link rel="stylesheet" href="@Url.Content("~/Content/mytemplate/")css/responsive.css">

    <!-- Js -->
    <script src="@Url.Content("~/Content/mytemplate/")js/vendor/modernizr-2.6.2.min.js"></script>
    <script src="@Url.Content("~/Content/mytemplate/")//ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.10.2.min.js"><\/script>')</script>
    <script src="@Url.Content("~/Content/mytemplate/")js/bootstrap.min.js"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/bootstrap.min.js"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/owl.carousel.min.js"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/plugins.js"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/min/waypoints.min.js"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/jquery.counterup.js"></script>
    <!-- Google Map -->
    <script src="@Url.Content("~/Content/mytemplate/")https://maps.googleapis.com/maps/api/js"></script>
    <script src="@Url.Content("~/Content/mytemplate/")js/google-map-init.js"></script>

    <script src="@Url.Content("~/Content/mytemplate/")js/main.js"></script>

</head>
<body>
    <!-- Header Start -->
    <header>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <!-- header Nav Start -->
                    <nav class="navbar navbar-default">
                        <div class="container-fluid">
                            <!-- Brand and toggle get grouped for better mobile display -->
                            <div class="navbar-header">
                                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                                    <span class="sr-only">Toggle navigation</span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                    <span class="icon-bar"></span>
                                </button>


                                <img src="~/Content/mytemplate/img/logo.png" />

                            </div>
                            <!-- Collect the nav links, forms, and other content for toggling -->
                            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                                <ul class="nav navbar-nav navbar-right">

                                    <li>@Html.ActionLink("Works", "Works")</li>
                                    <li>@Html.ActionLink("Book a Car", "Book")</li>
                                    <li>@Html.ActionLink("Logout", "Login")</li>

                                </ul>
                            </div><!-- /.navbar-collapse -->
                        </div><!-- /.container-fluid -->
                    </nav>
                </div>
            </div>
        </div>
    </header><!-- header close -->
    <!-- Slider Start -->
    <section id="global-header">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="block">
                        <h1>Add Details</h1>
                        <p>Don’t just take our word for it. Check out some of our latest work.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- contact form start -->
    <section id="contact-form">
        <div class="container">
            <div class="row">
                <div class="col-md-6 col-sm-12">
                    <div class="block">

                    </div>
                </div>
                <div class="col-md-6 col-sm-12">
                    <div class="block">

                    </div>
                </div>
            </div>
            <div id="contact-box" class="row">
                <div class="col-md-6 col-sm-12">
                    <div class="block">
                        <div align="center">
                            <form action="@Url.Action("Landing", "Home" )" method="post">

                                <h1>Schedule</h1>
                                <label for="">From</label>
                                <input type="date" name="fromdate" id="fromdate" required>

                                <label for="">To</label>
                                <input type="date" name="todate" id="todate" required>

                                <p><input type="submit" class="btn" value="Submit" onclick="javascript:return Validate();" /></p>


                                <p class="errormeaasge" id="perrormsg"></p>

                            </form>

                                 <script>
                                        function Validate()
                                        {
                                            var x =$("#Email").val();
                                            var y =$("#Password").val();
                                            var validationmessage = '';

                                            if (x == "") {
                                                validationmessage = "Username must be filled";
                                                $("#perrormsg").html(validationmessage);
                                                return false;
                                            }


                                  if (y == "") {
                                      validationmessage = "Password must be filled";
                                      $("#perrormsg").html(validationmessage);
                                   return false
                                  }
                                     return true
                                     }



                                 </script>

                        </div>
                    </div>
                </div>

            </div>
        </div>




    </section>
    <!-- Call to action Start -->
    <section id="call-to-action">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="block">
                        <h2>We design delightful digital experiences.</h2>
                        <p>Read more about what we do and our philosophy of design. Judge for yourself The work and results we’ve achieved for other clients, and meet our highly experienced Team who just love to design.</p>

                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- footer Start -->
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="footer-manu">

                    </div>
                    <p>Copyright &copy; Crafted by <a href="https://dcrazed.com/">SoftNice</a>.</p>
                </div>
            </div>
        </div>
    </footer>


</body>
</html>